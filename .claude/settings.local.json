      "*",
      "Bash(git add:*)",
      "Bash(mkdir:*)",
      "Bash(npx prisma generate:*)",
      "Bash(npm install)",
      "Bash(npm install:*)",
      "Bash(rm:*)",
      "Bash(npm run test:*)",
      "Bash(npm run type-check:*)",
