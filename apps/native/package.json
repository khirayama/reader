{
  "name": "@rss-reader/native",
  "version": "0.1.0",
  "private": true,
  "description": "RSS Reader Native Application",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build": "expo export",
    "dev": "expo start",
    "setup": "echo 'Native app: No setup required (using localhost:3001 for development)'",
    "lint": "biome lint .",
    "format": "biome format --write .",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "clean": "expo r -c"
  },
  "dependencies": {
    "@expo/metro-runtime": "~5.0.4",
    "@hookform/resolvers": "^3.9.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-navigation/bottom-tabs": "^7.3.14",
    "@react-navigation/drawer": "^7.4.1",
    "@react-navigation/native": "^7.1.10",
    "@react-navigation/stack": "^7.3.3",
    "@rss-reader/sdk": "file:../../packages/sdk",
    "axios": "^1.7.2",
    "expo": "53.0.11",
    "expo-document-picker": "^13.1.5",
    "expo-file-system": "^18.1.10",
    "expo-localization": "^16.1.5",
    "expo-sharing": "^13.1.5",
    "expo-status-bar": "~2.2.3",
    "i18next": "^23.12.2",
    "metro": "^0.82.4",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.52.1",
    "react-i18next": "^15.0.0",
    "react-native": "0.79.3",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-localize": "^3.2.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "^4.11.1",
    "react-native-web": "^0.20.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@types/node": "^20.14.12",
    "@types/react": "^19.0.0",
    "@vitest/coverage-v8": "^2.0.5",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
  }
