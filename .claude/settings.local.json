{
  "permissions": {
    "allow": [
      "*",
      "Bash",
      "Bash(git add:*)",
      "Bash(mkdir:*)",
      "Bash(npx prisma generate:*)",
      "Bash(npm install)",
      "Bash(npm install:*)",
      "Bash(rm:*)",
      "Bash(npm run test:*)",
      "Bash(npm run type-check:*)",
      "Bash(npm run lint)",
      "Bash(npm run build:*)",
      "Bash(git commit:*)",
      "Bash(npx prisma migrate dev:*)",
      "Bash(npx prisma db push:*)",
      "Bash(npx turbo run:*)",
      "Bash(npx tsc:*)",
      "Bash(mv:*)"
